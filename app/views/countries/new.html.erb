<div class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
      <h1>Create country </h1>
      <%= simple_form_for @country do |c| %>
        <%= c.input :code %>
        <%= c.input :label %>
        <%= c.input :active %>
        <%= c.button :submit %>
      <% end %>
    </div>
  </div>
</div>
